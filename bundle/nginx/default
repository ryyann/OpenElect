#
# Nginx default config file
#

upstream node {server 172.17.0.17:8080;}

server {
  error_log /dev/stderr;
  access_log /dev/stdout;
  server_name  localhost;
  listen 80;
  root /data;
  index index.html;
  location / {
    proxy_set_header X-Forwarded-Host nginx;
    proxy_pass http://node;
  }
}

